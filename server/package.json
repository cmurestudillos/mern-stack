  "keywords": [
    "node.js",
    "randomuser.me.",
    "vue.js",
    "mevn",
    "mongoDB",
    "express.js"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "mongoose": "^6.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
