{
  "name": "stack-api-randomuser-backend",
  "version": "1.0.0",
  "author": "Carlos Mur",
  "license": "MIT",  
  "description": "Backend usando la api \"https://randomuser.me/\"",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "npm run copy-env-dev",
    "prd": "npm run copy-env-prd",
    "copy-env-dev": "cp .env.development .env",
    "copy-env-prd": "cp .env.production .env",
    "deploy": "gcloud app deploy && gcloud app browse -s stack-api-randomuser"
